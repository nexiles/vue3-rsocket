{
  "name": "vue3-rsocket",
  "version": "0.2.1",
  "description": "Vue3 library for rsocket-js https://github.com/rsocket/rsocket-js",
  "main": "dist/vue3-rsocket.js",
  "module": "dist/vue3-rsocket.mjs",
  "typings": "dist/vue3-rsocket.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nexiles/vue3-rsocket"
  },
  "author": "nexiles GmbH",
  "contributors": [
    {
      "name": "Jakob Huber",
      "email": "jakob.huber@nexiles.com",
      "url": "https://github.com/gh7531"
    },
    {
      "name": "Samuel Frei",
      "email": "samuel.frei@nexiles.com",
      "url": "https://github.com/s-frei/"
    }
  ],
  "license": "MIT",
  "private": false,
  "watch": {
    "cleanbuild": {
      "patterns": ["src"],
      "extensions": "ts",
      "quiet": false
    }
  },
  "scripts": {
    "watch": "npm-watch",
    "clean": "rm -rf dist/",
    "build": "rollup -c",
    "cleanbuild": "yarn clean && yarn build",
    "dev": "rollup -c -w"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "rsocket-core": "^0.0.27",
    "rsocket-websocket-client": "^0.0.27",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/node": "^18.0.6",
    "@types/rsocket-core": "^0.0.7",
    "@types/rsocket-websocket-client": "^0.0.4",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "esbuild": "^0.14.49",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "npm-watch": "^0.11.0",
    "prettier": "2.7.1",
    "rollup": "^2.77.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-esbuild": "^4.9.1",
    "typescript": "^4.7.4"
  }
